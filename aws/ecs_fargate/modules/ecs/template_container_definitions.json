[
  {
    "name": "private-location-worker-service",
    "image": "datadog/synthetics-private-location-worker:latest",
    "command": [
      "--site='...'",
      "--accessKey='...'",
      "--datadogApiKey='...'",
      "--secretAccessKey='...'",
      "--privateKey='-----BEGIN RSA PRIVATE KEY----- ... -----END RSA PRIVATE KEY-----'",
      "--publicKey.pem='-----BEGIN PUBLIC KEY----- ... -----END PUBLIC KEY-----'",
      "--publicKey.fingerprint='...'"
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group" : "/ecs/private-locations",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "ecs-private-locations"
      }
    },
    "healthCheck": {
      "retries": 3,
      "command": [
        "CMD-SHELL", "/bin/sh -c '[ $(expr $(cat /tmp/liveness.date) + 300000) -gt $(date +%s%3N) ]'"
      ],
      "timeout": 2,
      "interval": 10,
      "startPeriod": 30
    },
    "compatibilities": [
      "EC2",
      "FARGATE"
    ]
  }
]
